        "  li_copy=li1[:]\n",
        "  return li_copy\n",
        "\n",
        "li1=[4,8,10,15,18]\n",
        "li2=cloning(li1)\n",
        "print(\"original list:\",li1)\n",
        "print(\"after cloning:\",li2)  "
      ],
      "execution_count": 5,
      "outputs": [
