        "def remove(duplicate):\n",
        "  final_list=[]\n",
        "  for num in duplicate:\n",
        "    final_list.append(num)\n",
        "  return final_list\n",
        "\n",
        "duplicate=[2,4,10,5,20,4] \n",
        "print(remove(duplicate))      "
      ],
      "execution_count": 3,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "[2, 4, 10, 5, 20, 4]\n"
          ],
          "name": "stdout"
        }
      ]
